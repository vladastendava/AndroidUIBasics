<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.endava.bootcamp.androiduibasics"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LinearLayoutHorizontalActivity" />
        <activity android:name=".LinearLayoutVerticalActivity" />
        <activity android:name=".NavigationActivity" />
        <activity android:name=".DialogsActivity" />
        <activity android:name=".RelativeLayoutActivity" />
        <activity android:name=".ListViewActivity" />
        <activity android:name=".StylesActivity" />
        <activity android:name=".LayoutsCombinationActivity" />
        <activity android:name=".ReadDataActivity" />
        <activity android:name=".ResultGenerationActivity"></activity>
    </application>

</manifest>